<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.FXML_VlozProcesController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label text="GPS X" />
            <TextField fx:id="textFieldX" />
            <Label text="GPS Y" />
            <TextField fx:id="textFieldY" />
            <Label text="Název Zámku" />
            <TextField fx:id="textFieldNazevZamku">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnOk" mnemonicParsing="false" onAction="#onActBtnOk" text="Ok" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onActBtnCancel" text="Cancel" />
         </children>
      </VBox>
   </children>
</AnchorPane>
